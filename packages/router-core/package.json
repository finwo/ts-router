{
  "name": "@finwo/router",
  "version": "0.5.2",
  "description": "TypeScript controller decorators",
  "main": "./lib/cjs/index.js",
  "types": "./lib/cjs/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/esm/index.d.ts",
        "default": "./lib/esm/index.mjs"
      },
      "require": {
        "types": "./lib/cjs/index.d.ts",
        "default": "./lib/cjs/index.js"
      }
    }
  },
  "scripts": {
    "linter": "eslint --config ../../config/eslint.config.js src",
    "test": "echo No tests yet",
    "clean": "rm -rf ./lib",
    "prebuild": "cp -r ../../config .",
    "build": "npm run clean && npm run build:esm && npm run build:cjs",
    "build:cjs": "tsc -p ./config/tsconfig.cjs.json",
    "build:esm": "tsc -p ./config/tsconfig.esm.json && mv lib/esm/index.js lib/esm/index.mjs"
  },
  "files": [
    "package.json",
    "README.md",
    "LICENSE",
    "lib/cjs/",
    "lib/esm/"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/finwo/ts-router.git"
  },
  "author": "Robin Bron <robin@finwo.nl>",
  "license": "MIT",
  "homepage": "https://github.com/finwo/ts-router#readme",
  "bugs": {
    "url": "https://github.com/finwo/ts-router/issues"
  },
  "funding": {
    "url": "https://github.com/sponsors/finwo"
  },
  "devDependencies": {
    "@types/node": "^18.15.3",
    "@types/tap": "^15.0.8",
    "@typescript-eslint/parser": "^8.40.0",
    "eslint": "^9.33.0",
    "tap": "^16.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.x"
  },
  "peerDependencies": {
    "@finwo/di": "^1.2.3",
    "reflect-metadata": "^0.1.13"
  }
}
